<h1>Listing accounts</h1>


  <div class="row">

    <div class="col-md-2">Name</div>
    <div class="col-md-2">Fullname</div>
    <div class="col-md-2"> Phone</div>
    <div class="col-md-2"> Email</div>
    <div class="col-md-2"></div>
      <div class="col-md-2"></div>
</div>
<br/ style="b">
    <% @accounts.each do |account| %>
      <div class="row">
      <div class="col-md-2"><%= account.name %></div>
      <div class="col-md-2"><%= account.fullName %></div>
      <div class="col-md-2"><%= account.phone %></div>
      <div class="col-md-2"><%= account.email %></div>
      <div class="col-md-2"><%= link_to 'Show', account %></div>
      <div class="col-md-1"><%= link_to 'Edit', edit_account_path(account) %></div>
      <div class="col-md-1"><%= link_to 'Destroy', account, method: :delete, data: { confirm: 'Are you sure?' } %></div>

          </div>
    <% end %>


<br>

<%= link_to 'New Account', new_account_path %>
